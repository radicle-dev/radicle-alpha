(ns prelude/list "Functions for creating lists. See also `prelude/seq`.")

(require prelude/test-macro ['test])
(require prelude/basic [])
(require prelude/bool [])

(def nil "The empty list." (list))

(def range
  "Returns a list with all integers from `from` to `to`, inclusive."
  (fn [from to]
    (if (eq? from to)
      (list to)
      (cons from (range (+ 1 from) to)))))

(test "range"
  [ (range 1 5) ==> (1 2 3 4 5) ]
)
